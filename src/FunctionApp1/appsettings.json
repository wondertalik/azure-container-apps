{
  "Logging": {
    "ApplicationInsights": {
      "SamplingSettings": {
        "IsEnabled": true,
        "MaxTelemetryItemsPerSecond": 20,
        "ExcludedTypes": "Request"
      },
      "EnableLiveMetricsFilters": true
    },
    "LogLevel": {
      "Default": "Information",
      "Host.Results": "Information",
      "Function": "Information",
      "Host.Aggregator": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}